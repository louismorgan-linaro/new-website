---
import "../styles/global.scss";
export async function getStaticPaths() {
  const pages = await Astro.glob("../content/pages/**/*.md");
  return pages.map((page) => ({
    params: {
      slug: page.frontmatter.slug,
    },
    props: {
      page,
    },
  }));
}

const { Content } = Astro.props.page;
---

<Content />
